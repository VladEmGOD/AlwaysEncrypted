@model IList<AlwaysEncrypted.DataAccess.UserDTO>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table align-content-center">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
    </tr>
    @for (int i = 0; i < Model.Count(); i++)
    {
        <tr>
            <td>@Model[i].Id</td>
            <td>@Model[i].Name</td>
            <td>@Model[i].Email</td>
        </tr>
    }
</table>
